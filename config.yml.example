openshift_setup_client_version: v3.11
openshift_setup_interface: docker0
openshift_setup_user: admin
openshift_setup_user_password: admin

ec2_install: true
ec2_force_terminate_instances: false

ec2_instance_type: m4.large
ec2_key: libra
ec2_private_key_file: ~/.ssh/libra.pem
ec2_subnet_cidr: 10.0.1.0/24
ec2_vpc_cidr: 10.0.0.0/16
ec2_vpc_name: origin3-dev

# additional_cors_allowed_origins - appends to the default
# corsAllowedOrigins master-config setting, whitelisting requests from
# alternative domains
# Ex:
#additional_cors_allowed_origins:
#  - //*.mg.dog8code.com
